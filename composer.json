{
    "name": "gonmt/fiserv-arg",
    "type": "library",
    "description": "Fiserv Argentina driver for the Omnipay payment processing library",
    "keywords": [
        "first data",
        "firstdata",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "fiserv"
    ],
    "homepage": "https://github.com/gonmt/omnipay-fiserv-arg.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Macneil",
            "email": "adrian@adrianmacneil.com"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/thephpleague/omnipay-firstdata/contributors"
        },
        {
            "name": "Gonzalo Torres",
            "email": "torresgonzalom@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\FiservArg\\" : "src/" }
    },
    "require": {
        "php": "^5.6|^7",
        "omnipay/common": "^3.1",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "require-dev": {
        "omnipay/tests": "~3.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
